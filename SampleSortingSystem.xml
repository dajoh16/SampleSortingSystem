int[0,N] len;


// Put an element at the end of the queue
void enqueue(Sample element)
{
        list[len++] = element;
}

// Remove the front element of the queue
void dequeue()
{
        int i = 0;
        len -= 1;
        while (i &lt; len)
        {
                list[i] = list[i + 1];
                i++;
        }
        list[i] = 0;
}

// Returns the front element of the queue
Sample front()
{
   return list[0];
}

// Returns the last element of the queue
Sample tail()
{
   return list[len - 1];
